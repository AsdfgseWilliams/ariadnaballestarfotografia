---
import { getLangFromUrl, useTranslations } from "src/i18n/utils";
import Layout from "src/layouts/Layout.astro";
import CTA from "src/sections/CTA.astro";
import Hero from "src/sections/Hero.astro";
import Opiniones from "src/sections/Opiniones.astro";
import CTA3 from "src/sections/CTA3.astro";
import Servicios from "src/sections/Servicios.astro";
import Sobremi from "src/sections/Sobremi.astro";
import Trabajos from "src/sections/Trabajos.astro";
const lang = getLangFromUrl(Astro.url);
const t = useTranslations(lang);
---

<Layout
	title={t('layout.index.title')}
	description={t('layout.index.description')}
	keywords={t('layout.index.keywords')}
>
	<Hero  />
	<Sobremi  />
	<Servicios  />
	<CTA  />
	<Trabajos  />
	<Opiniones  />
	<CTA3  />
</Layout>

<style></style>
